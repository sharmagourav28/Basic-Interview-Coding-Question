#include<iostream>
using namespace std;
void perfect(int n){
    int sum = 0;
    for(int i = 1;i<=n;i++){
        if(n%i==0){
            sum += i;
        }
    }
    if(sum == n){
        cout<<n<<" perfect number "<<endl;
    }
    else{
        cout<<n<<" Not perfect number"<<endl;
    }
}
int main(){
    int n;
    cin>>n;
    perfect(n);
    
}
